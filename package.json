{
  "name": "attractions",
  "version": "3.7.0",
  "description": "A UI kit for Svelte",
  "homepage": "https://illright.github.io/attractions/",
  "bugs": "https://github.com/illright/attractions/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/illright/attractions.git",
    "directory": "attractions"
  },
  "keywords": [
    "svelte",
    "ui"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint . --ext \".js,.ts,.svelte\"",
    "format": "prettier --write --plugin-search-dir=. .",
    "prepublishOnly": "node --unhandled-rejections=strict check-version.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0-next.37",
    "@sveltejs/kit": "1.0.0-next.405",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "lint-staged": "^13.0.3",
    "mdsvex": "^0.10.6",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "remark-heading-id": "^1.0.0",
    "sass": "^1.54.5",
    "stylelint": "^14.11.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-feather-icons": "^4.0.0",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.14",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.9"
  },
  "type": "module"
}
