{
  "name": "attractions",
  "version": "3.7.0",
  "description": "A UI kit for Svelte",
  "homepage": "https://illright.github.io/attractions/",
  "bugs": "https://github.com/illright/attractions/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/illright/attractions.git",
    "directory": "attractions"
  },
  "keywords": [
    "svelte",
    "ui"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint . --ext \".js,.ts,.svelte\"",
    "format": "prettier --write --plugin-search-dir=. .",
    "prepublishOnly": "node --unhandled-rejections=strict check-version.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0",
    "@sveltejs/kit": "1.0.1",
    "@sveltejs/package": "1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "lint-staged": "^13.1.0",
    "mdsvex": "^0.10.6",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "remark-heading-id": "^1.0.0",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^29.0.0",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "svelte-feather-icons": "^4.0.0",
    "svelte-preprocess": "^5.0.0",
    "svelte2tsx": "^0.6.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  },
  "type": "module"
}
