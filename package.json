{
  "name": "attractions",
  "version": "3.7.0",
  "description": "A UI kit for Svelte",
  "homepage": "https://illright.github.io/attractions/",
  "bugs": "https://github.com/illright/attractions/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/illright/attractions.git",
    "directory": "attractions"
  },
  "keywords": [
    "svelte",
    "ui"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "prepublishOnly": "node --unhandled-rejections=strict check-version.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.34",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "lint-staged": "^13.0.3",
    "mdsvex": "^0.10.6",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "remark-heading-id": "^1.0.0",
    "sass": "^1.53.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-feather-icons": "^4.0.0",
    "svelte-preprocess": "^4.10.6",
    "svelte2tsx": "^0.5.10",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.13"
  },
  "type": "module"
}
