{
  "name": "attractions",
  "version": "3.0.0",
  "description": "A UI kit for Svelte",
  "homepage": "https://illright.github.io/attractions/",
  "bugs": "https://github.com/illright/attractions/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/illright/attractions.git",
    "directory": "attractions"
  },
  "svelte": "index.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "unpkg": "dist/custom-elements.js",
  "sideEffects": [
    "./dist/custom-elements.js"
  ],
  "keywords": [
    "svelte",
    "ui"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --format=pretty . --ext '.svelte,.js'; stylelint .",
    "lint:fix": "eslint --format=pretty --fix . --ext '.svelte,.js,.json'; stylelint --fix .",
    "prepublishOnly": "yarn build && node ensure-verison-unpublished $(yarn info attractions version --silent)"
  },
  "files": [
    "*"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "prettier-plugin-svelte": "^2.0.3",
    "rollup": "^2.36.1",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.2",
    "stylelint": "^13.6.1",
    "svelte": "^3.31.2",
    "svelte-preprocess": "^4.5.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "postcss": ">=7 <9",
    "sass": "^1.30.0",
    "svelte": "^3.29.1",
    "svelte-preprocess": "^4.0.10"
  }
}
